{
    "builds": [
      {
        "src": "dashboard/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [{
        "builds": [
          {
            "src": "dashboard/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.9"
            },
            "output": "dashboard/dist"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "dashboard/wsgi.py"
          }
        ]
      }
      
      {
        "src": "/(.*)",
        "dest": "dashboard/wsgi.py"
      }
    ]
  }
  